{
  "name": "@alzera/qr-scanner",
  "version": "1.0.4",
  "description": "QR code scanner component for React apps",
  "license": "MIT",
  "author": "Alzera Cita <alzeracita23@gmail.com>",
  "keywords": [
    "react",
    "qr",
    "qrcode",
    "scanner",
    "reader"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alzera/qr-scanner.git"
  },
  "bugs": {
    "url": "https://github.com/Alzera/qr-scanner/issues"
  },
  "homepage": "https://github.com/Alzera/qr-scanner#readme",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./react": {
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.cjs.js",
      "default": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./vue": {
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.cjs.js",
      "default": "./dist/vue/index.js",
      "types": "./dist/vue/index.d.ts"
    },
    "./vue/qr-scanner.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "npx vite-bundle-visualizer"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "peerDependencies": {
    "barcode-detector": "^2.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vue": "^3.4.21"
  }
}
